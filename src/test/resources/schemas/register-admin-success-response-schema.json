{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Register Admin Success Response",
  "type": "object",
  "required": ["status", "data","otp"],
  "properties": {
    "status": {
      "type": "boolean",
      "const": true
    },
    "data": {
      "oneOf": [
        { "type": "array" },
        { "type": "object" },
        { "type": "string" },
        { "type": "null" }
      ]
    },
    "code": { "type": ["integer", "null"] },
    "error": { "type": ["string", "null"] },
    "description": { "type": ["string", "null"] }
  },
  "otp": {
	"type": "string"
  },
  "additionalProperties": true
}
